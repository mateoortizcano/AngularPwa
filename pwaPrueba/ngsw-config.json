{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "datagroups": [
    {
      "name": "RedditAPI",
      "urls": [
        "https://reddit.com/r/**"
      ],
      "cacheConfig": {
        "srategy": "freshness",
        "maxAge": "1d",
        "maxSize": 1000,
        "timeout": "3s"
      }
    }
  ]
}
